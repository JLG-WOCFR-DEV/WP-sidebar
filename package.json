{
  "name": "wp-sidebar",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:js": "jest",
    "test:php": "composer test",
    "build": "npm run build:assets",
    "build:assets": "wp-scripts build sidebar-jlg/assets/js/blocks/sidebar-search.js sidebar-jlg/assets/js/blocks/sidebar-search-view.js sidebar-jlg/assets/js/admin-app.tsx --output-path=sidebar-jlg/assets/build",
    "start:assets": "wp-scripts start sidebar-jlg/assets/js/blocks/sidebar-search.js sidebar-jlg/assets/js/blocks/sidebar-search-view.js sidebar-jlg/assets/js/admin-app.tsx --output-path=sidebar-jlg/assets/build",
    "audit:accessibility": "pa11y --config pa11y.config.json docs/demo.html"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.19",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@wordpress/scripts": "^27.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jquery": "^3.7.1",
    "pa11y": "^9.0.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "zustand": "^4.5.2"
  }
}
